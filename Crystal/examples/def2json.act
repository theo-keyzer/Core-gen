----------------------------------------------------------------
Actor main
----------------------------------------------------------------

Cs {

All Comp comps . . .  

C
C }

----------------------------------------------------------------
Actor comps Comp parent = .
----------------------------------------------------------------

C ${.1.,}
C ${.arg}  "${name} ": 
Cs ${.arg}  [

All ${name} list . . . ${.arg}   

C
Cs ${.arg}  ]

----------------------------------------------------------------
Actor list
----------------------------------------------------------------

C ${.1.,}
C ${.arg} {

Its .comps.Element ele . . . ${.arg}  
Its Child list2 . . . ${.arg}  

C
Cs ${.arg} }

----------------------------------------------------------------
Actor ele
----------------------------------------------------------------

Var a = $${.list.${name} }

C ${.1.,}
Cs ${.arg}  "${name} ": "${a}$"


----------------------------------------------------------------
Actor list2
----------------------------------------------------------------

C ,
C ${.arg}  "${comp} ": 
Cs ${.arg}  [

All Comp.${comp} comps2 . . . ${.arg}   

C
Cs ${.arg}  ]

----------------------------------------------------------------
Actor comps2
----------------------------------------------------------------

C ${.1.,}
C ${.arg} {

Its Element 2ele . . . ${.arg}  
Its .list2.Child list2 . . . ${.arg}  

C
Cs ${.arg} }

----------------------------------------------------------------
Actor 2ele
----------------------------------------------------------------

Var a = $${.list2.${name} }

C ${.1.,}
Cs ${.arg}  "${name} ": "${a}$"


