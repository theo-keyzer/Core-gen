----------------------------------------------------------------
Actor main
----------------------------------------------------------------

All Comp comp

----------------------------------------------------------------
Actor comp Comp
----------------------------------------------------------------

C
C Comp ${name} ${nop} ${parent} ${find:eol} ${doc}

Its Element element
Its Join ele_join

C

Its Join ref_join

----------------------------------------------------------------
Actor element Element
----------------------------------------------------------------

C  Element ${name} ${mw} ${mw2} ${pad} ${doc}

----------------------------------------------------------------
Actor ele_join Join
----------------------------------------------------------------

-C  Element ${element} 

----------------------------------------------------------------
Actor ref_join Join dir = from
----------------------------------------------------------------

C  Element ${element}    U0 ${comp} 
C  Refu    ${element} ${comp}  ${element2:l} ${comp2:c} ${element3:l}

Break

----------------------------------------------------------------
Actor ref_join Join using = using
----------------------------------------------------------------

C  Element ${element} L1 ${comp}
C  Ref2 ${element} ${comp} ${element2:l} ?

Break

----------------------------------------------------------------
Actor ref_join Join
----------------------------------------------------------------

All Comp ref_join_comp


----------------------------------------------------------------
Actor ref_join_comp Comp name = ${.ref_join.comp}
Actor ref_join_comp Comp parent &= .
----------------------------------------------------------------

C  Element ${.ref_join.element} R1 ${.ref_join.comp}
C  Ref ${.ref_join.element} ${.ref_join.comp} .

Break

----------------------------------------------------------------
Actor ref_join_comp Comp name = ${.ref_join.comp}
----------------------------------------------------------------

Its parent parent

----------------------------------------------------------------
Actor parent Comp name = ${.ref_join_comp.parent}
----------------------------------------------------------------

C  Element ${.ref_join.element:l} F1 ${.ref_join_comp.name:c}
C  Ref ${.ref_join.element:l} ${.ref_join_comp.name:c} .

Break

----------------------------------------------------------------
Actor parent Comp parent = .
----------------------------------------------------------------

C  Element ${name:l} R1 ${name:c}
C  Ref ${name:l} ${name:c} .
C  Element ${.ref_join.element} L1 ${.ref_join.comp}
C  Ref2 ${.ref_join.element} ${.ref_join.comp} ${name:l} ?

Break

----------------------------------------------------------------
Actor parent
----------------------------------------------------------------

C  Element ${parent.name:l} R1 ${parent.name:c}
C  Ref ${parent.name:l} ${parent.name:c} .
C  Element ${name:l} L1 ${name:c}
C  Ref2 ${name:l} ${name:c} ${parent.name:l} .
C  Element ${.ref_join.element} L1 ${.ref_join.comp}
C  Ref2 ${.ref_join.element} ${.ref_join.comp} ${name:l} ?

