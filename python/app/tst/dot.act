----------------------------------------------------------------
Actor main
----------------------------------------------------------------

C digraph unit {
C node [shape=box];

All Comp comp

C }
C

----------------------------------------------------------------
Actor comp Comp
----------------------------------------------------------------

C "${name}"

Its Element ele
Its parent parent

----------------------------------------------------------------
Actor parent Comp
----------------------------------------------------------------

C "${.comp.name}" -> "${name}" [label = "parent"];
C "${name}" -> "${.comp.name}" [label = "${.comp.name}"];

----------------------------------------------------------------
Actor ele Element mw in R1,RS,F1,FS
----------------------------------------------------------------

C "${.comp.name}" -> "${Ref_element.comp}" [label = "${name}"];
C "${Ref_element.comp}" -> "${.comp.name}" [label = "${.comp.name}_${name}"];

----------------------------------------------------------------
Actor ele Element mw in L1,LS
----------------------------------------------------------------

C "${.comp.name}" -> "${Ref2_element.comp}" [label = "${name}"];
C "${Ref2_element.comp}" -> "${.comp.name}" [label = "${.comp.name}_${name}"];

----------------------------------------------------------------
Actor ele Element mw in U0
----------------------------------------------------------------

C "${.comp.name}" -> "${Refu_element.comp}" [label = "${name}"];


