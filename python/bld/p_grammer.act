----------------------------------------------------------------
Actor main .
----------------------------------------------------------------

C   ?start: lines
C
C   lines: line*
C
Cs   line: COMMENT | NL 

All Comp def

C

All Comp def2

C
C     %import common.CNAME -> C1_NAME
C     %import common.WORD -> C1_WORD
C     %import common.NEWLINE -> NL
C     %import common.WS_INLINE
C     %import common.WS
C    
C     COMMENT: /[-*][^\n]+/
C     V1_WORD: /[^\n]+/
C     P1_WORD: /[a-zA-z0-9.;*?\\\/]+/
C     %ignore WS_INLINE
C     %ignore WS
C

----------------------------------------------------------------
Actor def . parent = .
----------------------------------------------------------------

Cs  | ${name}l_def

----------------------------------------------------------------
Actor def2 .
----------------------------------------------------------------

Cs   ${name}l_def: "${name}c"

Its Element ele

Cs  NL

Du def3

C

----------------------------------------------------------------
Actor def3 .
----------------------------------------------------------------

Out delay

Cs  (COMMENT | NL

Its Comp_parent childs

Cs )*

----------------------------------------------------------------
Actor ele . mw in C1,R1,F1,L1,M1,N1,P1,Q1
----------------------------------------------------------------

Cs  P1_WORD?

----------------------------------------------------------------
Actor ele . mw in V1
----------------------------------------------------------------

Cs  V1_WORD?

----------------------------------------------------------------
Actor childs .
----------------------------------------------------------------

Cs  | ${name}l_def

